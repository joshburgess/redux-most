{
  "name": "redux-most",
  "version": "0.8.0",
  "description": "Most.js based middleware for Redux. Handle async actions with monadic streams and reactive programming.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=test ava --tap | tap-diff",
    "tdd": "cross-env BABEL_ENV=test ava --watch",
    "safety-check": "npm run lint && npm run test",
    "build:cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "rimraf dist && cross-env BABEL_ENV=umd webpack --config webpack.config.babel.js",
    "build": "npm run build:cjs && npm run build:es && npm run build:umd",
    "prepublish": "npm run safety-check && npm run build"
  },
  "repository": "joshburgess/redux-most",
  "keywords": [
    "action",
    "async",
    "asynchronous",
    "fluent",
    "functional",
    "middleware",
    "monad",
    "monadic",
    "most",
    "most.js",
    "mostjs",
    "observable",
    "reactive",
    "reactive extensions",
    "reactive programming",
    "reactive streams",
    "redux",
    "redux-observable",
    "redux-saga",
    "rx",
    "rxjs",
    "saga",
    "sagas",
    "stream",
    "streams",
    "thunk"
  ],
  "author": {
    "name": "Josh Burgess",
    "email": "joshburgess.webdev@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshburgess/redux-most/issues"
  },
  "homepage": "https://github.com/joshburgess/redux-most#README.md",
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "peerDependencies": {
    "most": "1.*",
    "redux": "^4.0.1"
  },
  "dependencies": {
    "most-subject": "^6.0.0"
  },
  "devDependencies": {
    "@most/core": "^1.6.1",
    "@most/scheduler": "^1.3.0",
    "ava": "^0.22.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "eslint": "^4.6.1",
    "eslint-config-standard-pure-fp": "^2.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-better": "0.1.5",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "nyc": "^11.1.0",
    "rimraf": "^2.5.4",
    "sinon": "^3.2.1",
    "tap-diff": "^0.1.1",
    "typescript": "^3.9.10",
    "typings": "^2.1.0",
    "webpack": "^3.5.5"
  },
  "npmName": "redux-most",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "semi": false
  }
}
